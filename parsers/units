kg m s mol K A
# gram meter second mol kelvin coulomb

# Length prefixes
mm = 0.001 m
cm = 0.01 m
km = 1000 m

# Mass prefixes
g = 0.001 kg

# Time prefixes
min = 60 s
hr = 60 min

# Liquid volume
L = 0.001 m^3
mL = 0.001 L

# Concentration
M = 1 mol / L

# Force
N = 1 kg m / s^2
J = 1 N m
W = 1 J / s

kJ = 1000 J
kW = 1000 W

# Pressure
Pa = 1 N / m^2
atm = 101325 Pa
torr = 133.322 Pa

# Electricity
C = 1 A s
V = 1 J / C
F = 1 C / V
O = 1 V / A
